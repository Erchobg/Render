local HWIDList = loadstring(game:HttpGet("https://raw.githubusercontent.com/SystemXVoid/VoidwareBConfig/main/whitelist2.lua", true))()
local HWID = game:GetService("RbxAnalyticsService"):GetClientId()


for i,v in pairs(HWIDList) do
    if v == HWID then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/SystemXVoid/VoidwareBConfig/main/chattags/TapewareOwner", true))()
        else
         loadstring(game:HttpGet("https://raw.githubusercontent.com/SystemXVoid/VoidwareBConfig/main/chattags/Standard", true))()
    end
    end
